<?xml version="1.0"?>
<block>
  <name>msg_to_file</name>
  <key>futebol_msg_to_file</key>
  <category>[futebol]</category>
  <import>import futebol</import>
  <make>futebol.msg_to_file()</make>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
	<sink>
		<name>in</name>
		<type>message</type>
	</sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
	<source>
		<name>out</name>
		<type>byte</type>
		<nports>1</nports>
	</source>
</block>
